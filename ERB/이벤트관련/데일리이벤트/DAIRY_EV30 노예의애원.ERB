#DIM 발생율

;DAILY_EV27 주인님의조교 검사
발생율 = 0
FOR COUNT, 1, CHARANUM
	SIF TALENT:COUNT:주인
		발생율 += 10
NEXT
SIF 발생율 >= 50
	발생율 = 50
IF 발생율 < RAND:100 + 1
	RETURN -1
ELSE
	CALL DAILY_EV27
	;당신에게 노예가 있는지 검사
	FOR COUNT, 1, CHARANUM
		SIF TALENT:COUNT:사망 || CFLAG:COUNT:출금
			CONTINUE
		SIF TALENT:COUNT:당신의노예 == 1
			LOCAL = 1
	NEXT
	IF LOCAL == 1
		SIF RAND:3 == 0
			PRINTFORML 당신의 노예가 기다리다 지쳐 떠난 모양이다...
	ENDIF
	;주인님의조교가 발생했으면 여기서 끝
	RETURN -1
ENDIF

;DAILY_EV30 노예의애원 검사
발생율 = 0
FOR COUNT, 1, CHARANUM
	SIF TALENT:COUNT:당신의노예&&(!TALENT:COUNT:사망 && !CFLAG:COUNT:출금)
		발생율 += 10
NEXT
SIF 발생율 >= 50
	발생율 = 50
IF 발생율 < RAND:100
	RETURN -1
ELSE
	CALL DAILY_EV30
	SIF RESULT == 3
		RETURN 1
ENDIF
RETURN -1



