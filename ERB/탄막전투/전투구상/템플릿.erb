[SKIPSTART]
;스킵 스타트랑 엔드는 지워주세요

;RETURN 하는 값이 2면 이벤트 발생, 그 이외 이면 이벤트 취소입니다

;일단은 가볍게 몇개만 넣어둠, 나중에 더 추가될지도 몰라

;파일 제목은 "캐릭터번호 캐릭터이름". 즉, 레이무의 경우 001 레이무.ERB인 셈

;         V  이거 {}는 빼야하는거 알지?
@T_KOJO_{캐릭터번호}(ARG,ARG:1)
;PRINTFORML 「」
SELECTCASE ARG
    CASE TK_존재판정
        ;포함된 이벤트의 경우 리턴값을 2로 해주세요, 제발!!!
        IF ARG:1 == TK_처형직전
            RETURN 0
        ELSEIF ARG:1 == TK_처형직후
            RETURN 0
        ELSEIF ARG:1 == TK_패배강간직전
            RETURN 0
        ELSEIF ARG:1 == TK_패배강간시작
            RETURN 0
        ELSEIF ARG:1 == TK_패배감금직전
            RETURN 0
        ELSEIF ARG:1 == TK_패배감금시작
            RETURN 0
        ELSEIF ARG:1 == TK_노예화직전
            RETURN 0
        ELSEIF ARG:1 == TK_노예화직후
            RETURN 0
        ELSEIF ARG:1 == TK_승리자비
            RETURN 0
        ELSEIF ARG:1 == TK_승리강간
            RETURN 0
        ELSEIF ARG:1 == TK_승리걷어참
            RETURN 0
        ELSEIF ARG:1 == TK_승리마무리
            RETURN 0
        ENDIF
        ;이건 건들지 말고
        RETURN 0
    CASE TK_처형직전
        ;처형 직전에 살려달라고 애원하는 이벤트
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
        CALL ASK_YN
        IF 1
            ;살린다
            ;여기에도 대사를 넣을수 있어
            RETURN 2
        ELSE
            ;죽인다
            ;여기에도 대사를 넣을수 있어
            RETURN 0
        ENDIF
    CASE TK_처형직후
        ;죽인후 일어나는 일, 대사, 근데 말할순 없으니 뭐...
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
    CASE TK_패배강간직전
        ;하지 말아달라는 애원의 이벤트
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
        IF 1
            ;멈춘다
            ;여기에도 대사를 넣을수 있어
            RETURN 2
        ELSE
            ;범한다
            ;여기에도 대사를 넣을수 있어
            RETURN 0
        ENDIF
    CASE TK_패배강간시작
        ;그런거 없고 당하기 시작하는 시점, 여기서 리턴값 1을 하면 도망가버렸다던가 그런류겠지?
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
        IF 1
            ;멈춘다
            ;여기에도 대사를 넣을수 있어
            RETURN 2
        ELSE
            ;범한다
            ;여기에도 대사를 넣을수 있어
            RETURN 0
        ENDIF
     CASE TK_패배감금직전
        ;하지 말아달라는 애원의 이벤트
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
        IF 1
            ;멈춘다
            ;여기에도 대사를 넣을수 있어
            RETURN 2
        ELSE
            ;진행한다
            ;여기에도 대사를 넣을수 있어
            RETURN 0
        ENDIF
    CASE TK_패배감금시작
        ;그런거 없고 당하기 시작하는 시점, 여기서 리턴값 1을 하면 도망가버렸다던가 그런류겠지?
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
        IF 1
            ;멈춘다
            ;여기에도 대사를 넣을수 있어
            RETURN 2
        ELSE
            ;범한다
            ;여기에도 대사를 넣을수 있어
            RETURN 0
        ENDIF
    CASE TK_노예화직전
        ;패배해서 노예당하기 직전, 딱히 변화는 줄 수 없다
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
        ;RETURN 0
    CASE TK_노예화직후
        ;노예화된 직후, 위의거랑 차이는 순서정도?
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
        ;RETURN 0
    CASE TK_승리자비
        ;상대(즉, 구상 주인)가 이겼지만 플레이어를 살려줌
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
        ;RETURN 0
    CASE TK_승리강간
        ;구상주가 이겼고, 플레이어를 강간
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
        ;RETURN 0
    CASE TK_승리걷어참
        ;구상주가 이겼고, 플레이어를 걷어참
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
        ;RETURN 0
    CASE TK_승리마무리
        ;구상주가 이겼고, 플레이어 확인사살함
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
        ;RETURN 0
ENDSELECT
[SKIPEND]