@공용스킬처리
;마스터도 포함
FOR LOCAL,0,CHARANUM
    ;이 안에 입력
    
NEXT
;마스터는 빼고
FOR LOCAL,1,CHARANUM
    ;이 안에 입력
    LOCAL:1 = 0
    FOR LOCAL:2,100,T_스킬한계값
        IF HAVE_T_SKILL(LOCAL,LOCAL:2)
            LOCAL:1 = 1
            BREAK
        ENDIF
    NEXT
    TALENT:LOCAL:정화 = 1
    TALENT:LOCAL:저주해제 = 1
    
    IF LOCAL:1 == 0
        TALENT:LOCAL:탄막소사 = 1
    ELSE
        TALENT:LOCAL:탄막소사 = 0
    ENDIF
NEXT

