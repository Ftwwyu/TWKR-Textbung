
@TSC_23
PRINTFORML 어떤걸 가공할까?
DRAWLINE
FOR LOCAL,1100,1500
    CALL 약물데이터(LOCAL)
    IF RESULT && D_NUM == LOCAL
        PRINTFORMC [{LOCAL,4,RIGHT}] %ITEMNAME:LOCAL,16,RIGHT% x({ITEM:LOCAL})
        LOCAL:1++
        IF LOCAL:1 == 3
            PRINTL
            LOCAL:1 = 0
        ENDIF
        RESULT = 0
    ENDIF
NEXT
PRINTL
DRAWLINE
PRINTFORML [999] 돌아간다
$LOOP
INPUT
IF RESULT == 999
	RETURN
ELSEIF RESULT <= 1100 + 약물리스트한계 && ITEM:RESULT
    LOCAL = RESULT
    CALL 약물데이터(LOCAL)
    IF RESULT == 1 && D_NUM == LOCAL
		PRINTFORML 어떻게 가공할까?
		LOCAL:2 = 0
		FOR LOCAL:1,1100,1100 + 약물리스트한계
			CALL 약물자식관계(LOCAL,LOCAL:1)
			IF RESULT
				PRINTFORM [{LOCAL:1,4,RIGHT}] %ITEMNAME:(LOCAL:1),16,RIGHT%　　　　　
				LOCAL:2++
				IF LOCAL:2 == 3
					PRINTL
					LOCAL:2 = 0
				ENDIF
				RESULT = 0
			ENDIF
		NEXT
		PRINTL
		DRAWLINE
		PRINTFORML [999] 돌아간다
		$LOOP1
		INPUT
		IF RESULT == 999
		ELSE
			LOCAL:1 = RESULT
			CALL 약물자식관계(LOCAL,LOCAL:1)
			IF RESULT
				ITEM:LOCAL --
				ITEM:(LOCAL:1)++
				PRINTFORML %조사처리(ITEMNAME:(LOCAL:1),"을")% 1개 만들었다
				TIME += 15
				DOWNBASE:MASTER:기력 += 150
				CALL AddEXP("조제경험",MASTER,1)
			ENDIF
		ENDIF
	ELSE
		GOTO LOOP
	ENDIF
ELSE
	GOTO LOOP
ENDIF
TIME += 30
RESTART


