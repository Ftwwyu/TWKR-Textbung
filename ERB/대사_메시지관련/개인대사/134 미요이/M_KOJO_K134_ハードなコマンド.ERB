;==================================================
;150,가슴の谷間を犯す
;==================================================
@M_KOJO_MESSAGE_COM_K134_150
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K134_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K134_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示, 1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「잠깐, 불쑥 들이대는 건 안 좋아요%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「가슴 안에 가득 싸주세요오%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「가슴 오나홀에 흥분해버리셨나요%UNICODE(0x2665) *1%점점 커지고 있어요오%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「정말! 너무 난폭해요오%UNICODE(0x2665) *1%」
		ENDIF
	;가슴3以上
	ELSEIF ABL:가슴 > 2
		IF RAND:3 == 0
			PRINTFORMW 「미요이의 가슴으로 기분 좋아져주세요오%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「응%UNICODE(0x2665) *1%가슴 기분 좋으신가요오%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「잔뜩 사정해주세요오%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「으응%UNICODE(0x2665) *1% 기분 좋으신가요오%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「잔뜩 가슴 범해주세요%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「너무 난폭하게 하지 마세요오%UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
ENDIF
RETURN 1
