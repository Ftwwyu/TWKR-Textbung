@M_KOJO_MESSAGE_COM_K99_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K99
	;시간정지中
	IF FLAG:시간정지
		PRINTFORMDL %CALLNAME:MASTER%는 등 뒤에서 토요히메의 엉덩이에 손을 뻗었다
		PRINTFORMDW 시간이 멈춰진 토요히메는 무저항인 채로 엉덩이를 어루만져지고 있다…
	;수면간中
	ELSEIF CFLAG:수면간
		PRINTFORMDL %CALLNAME:MASTER%는 누워서 자는 중인 토요히메의 엉덩이에 손을 뻗었다
		PRINTFORMDW 토요히메는 조금 뺨을 물들인 채 %CALLNAME:MASTER%가 하는 대로 되고 있다…
	;押し倒され
	ELSEIF CFLAG:우후후 == 2
		CALL TRAIN_MESSAGE
		RETURN 1
	;通常
	ELSE
		;연모
		IF TALENT:연모
			PRINTFORMDL %CALLNAME:MASTER%は\@ PREVCOM == 305 ? 무릎에 드러누운 채로 # 등 뒤로부터 \@토요히메의 엉덩이에 손을 뻗었다
			PRINTFORMDL 토요히메는 달아오른 한숨을 나직이 흘리면서 %CALLNAME:MASTER%가 하는 대로 맡겼다……
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「으응……HPH 그런 식으로 만지지만 말고……」","L"
					CALL HPH_PRINT,@"「좀 더 물건같이 다뤄도 좋은데요……？HPH」","L"
					PRINTFORMDL 토요히메는 소유물과 같이 취급해지는 것을 소망하는 것 같다
					PRINTFORMDL 그렇다면야 하고 %CALLNAME:MASTER%가 버릇없이 엉덩이를 덥석 움켜쥐어도
					PRINTFORMDW 그것이 소망이다라는 듯이 토요히메는 아무런 저항도 하지 않고, 달콤한 숨결만을 흘릴 뿐이었다
				CASE 1
					CALL HPH_PRINT,@"「귀하의 손대는 방법, 굉장히 엣치하기 때문에……HPH」","L"
					CALL HPH_PRINT,@"「하아……이상한 기분이 되어 버릴 것 같아……HPH」","L"
					PRINTFORMDW %CALLNAME:MASTER%는 토요히메의 복숭아 같은 엉덩이의 감촉을 마음껏 즐겼다
				CASE 2
					CALL HPH_PRINT,@"「아……하아……HPH 응、응응……HPH」","L"
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%……좀 더……HPH」","L"
					PRINTFORMDW 토요히메는 황홀한 표정으로 %CALLNAME:MASTER%의 성희롱을 받아들이고 있다
			ENDSELECT
			PALAM:윤활 += 500
		;사모
		ELSEIF TALENT:사모
			PRINTFORMDL %CALLNAME:MASTER%は\@ PREVCOM == 305 ? 무릎에 드러누운 채로 # 등 뒤로부터 \@토요히메의 엉덩이에 손을 뻗었다
			PRINTFORMDL 토요히메는 %CALLNAME:MASTER%에게 당하는 성희롱을 즐기고 있는 것 같다……
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「앗……좀、깜짝 놀랐잖아」
					PRINTFORML 「말해 준다면 분명하게 손대게 해 주는데」
					PRINTFORMDW %CALLNAME:MASTER%가 사후 승낙을 요청하자、토요히메는 쿡쿡 웃으며 엉덩이를 내밀었다……
				CASE 1
					PRINTFORML 「싫어ー치한이야ー범해져 버려요ー」
					PRINTFORMDL 토요히메는 김이 빠질 것 같은 단조로운 국어책 읽기 식의 도움을 구하고 있다
					PRINTFORML 「어때？ 그럴싸하지 않을까」
					PRINTFORMDW 이러니 저러니 해도 토요히메도 즐기고 있는 것 같다
				CASE 2
					PRINTFORML 「당신은 정말로 엉덩이를 좋아하는군요」
					PRINTFORML 「안 돼요？ 다른 아이에게 일 해서는」
					PRINTFORMDW 이제 익숙해져 버렸는지、%CALLNAME:MASTER%의 짖궃은 성희롱에도 토요히메는 전혀 동요하지 않았다
			ENDSELECT
		;それ以外
		ELSE
			PRINTFORMDL %CALLNAME:MASTER%は\@ PREVCOM == 305 ? 무릎에 드러누운 채로 # 등 뒤로부터 \@토요히메의 엉덩이에 손을 뻗었다
			PRINTFORMDL 토요히메는 쑥스러운 표정을 지으면서도 %CALLNAME:MASTER%가 하자는 대로 대주고 있다……
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「꺄악……」
					PRINTFORMDL %CALLNAME:MASTER%는 토요히메의 복숭아 같은 엉덩이를 추잡하게 더듬었다
					PRINTFORMW 「정말……몹쓸 사람이네」
				CASE 1
					PRINTFORML 「으응……안 돼요……이런 일……」
					PRINTFORMDL 입으로는 그렇게 말하는 토요히메지만 저항할 생각은 전혀 없는 것 같다
					PRINTFORMDW %CALLNAME:MASTER%는 그것을 좋을대로 받아들여 그녀의 엉덩이를 만족할 때까지 계속 어루만졌다
				CASE 2
					PRINTFORML 「저……기요？」
					PRINTFORML 「과연……그 저기、이런 일은……」
					PRINTFORMDL 토요히메는 곤란하다는 듯이 시선을 이리저리 돌린다
					PRINTFORMDW 뺨을 후려치는 것을 각오 하던 %CALLNAME:MASTER%는 조금 맥이 풀려버렸다
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RESETCOLOR
RETURN 1
