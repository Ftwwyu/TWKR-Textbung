;==================================================
@M_KOJO_SPEVENT_K99_2(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_2(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML
		RETURN 1
	ENDIF

ENDIF
RETURN 1

;==================================================
;デート帰り
;==================================================
@M_KOJO_SPEVENT_K99_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		;同棲
		IF CFLAG:MASTER:초기위치 == CFLAG:99:초기위치
			PRINTFORML 「자 그럼、이따가 봐요♪」
			PRINTFORMDW 토요히메는 먼저 방으로 돌아갔다
		;通常
		ELSE
			;연모
			IF TALENT:연모
				PRINTFORML 「음……다음에 또야。약속이야？」
				PRINTFORMDW 헤어질 때、토요히메는 %CALLNAME:MASTER%의 볼에 키스를 하고는 아쉬운 듯 떠나갔다
			;사모
			ELSEIF TALENT:사모
				PRINTFORML 「다음에 언제 데이트 갈까나」
				PRINTFORML 「내일이든、모레든……아、지금 당장 갈래？」
				PRINTFORMDW 오늘은 이만 끝내기로 하자。그렇게 말하자 토요히메는 아쉬움을 떨치지 못하고 있다
			;それ以外
			ELSE
				PRINTFORML 「오늘은 즐거웠어」
				PRINTFORML 「다음에 또 데려가줘」
				PRINTFORMDL 토요히메는 겉치레 말이 아니라、진심으로 그렇게 말하고 있다
				PRINTFORMDW ……라는 생각이 들었다
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;반발각인消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;반발각인2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;반발각인3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K99_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		$TOYOHIME_ENCOUNTER
		;既に会っている場合は非表示
		IF !TCVAR:99:오늘만났는지
			;연모
			IF TALENT:연모
				PRINTFORML 「%CALLNAME:MASTER%、이쪽이야 이쪽♪」
				PRINTFORMDW 토요히메는 %CALLNAME:MASTER%를 의식하며、때때로 손을 흔들었다
			;사모
			ELSEIF TALENT:사모
				PRINTFORM 「음、%CALLNAME:MASTER%。
				;TIME:2で朝昼晩を選択
				SELECTCASE TIME:2
				;朝
				CASE 1,2
					PRINTFORML 오늘은 빠르구나」
				;昼
				CASE 3,4
					PRINTFORML 오늘은 웬일이야？」
				;夜
				CASE 5,6,7
					PRINTFORML 저녁 식사는 벌써 끝냈다？」
				ENDSELECT
				PRINTFORMDW 토요히메는 %CALLNAME:MASTER%를 깨닫고、기쁜 듯이 미소지었다
			;それ以外
			ELSE
				PRINTFORML 「어머、%CALLNAME:MASTER%」
				PRINTFORMW 「마침 한가로운 시간을 주체할 수 없었어。괜찮다면 말벗이 되어 줄래？」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;既に会っている場合は非表示
		IF !TCVAR:99:오늘만났는지
			;연모
			IF TALENT:연모
				PRINTFORML 「아아、%CALLNAME:MASTER%。거기에 있었구나」
				PRINTFORMW 「계속 널 찾고 있었다고？」
			;사모
			ELSEIF TALENT:사모
				PRINTFORML 「산책을 하다가、우연히 이곳에 당도한거야」
				PRINTFORMW 「후훗、무슨 운명 같은게 느껴지지 않니？」
			;それ以外
			ELSE
				PRINTFORML 「어머어머、%CALLNAME:MASTER%。우연이네」
				PRINTFORMW 「뭐하고 있었어？ 나도 끼워줘라♪」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		GOTO TOYOHIME_ENCOUNTER
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1に데이트상대
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;既に会っている場合は非表示
		IF !TCVAR:99:오늘만났는지
			;永琳とデート
			IF FLAG:데이트상대 == 72
				PRINTFORML 「……야고코로님도 의외로……」
				PRINTFORML 「아아、아니아니에요！아무것도 아닙니다」
				PRINTFORML 「그럼、다녀오세요」
			;依姫とデート
			ELSEIF FLAG:데이트상대 == 98
				PRINTFORML 「어머어머、귀녀도 또한 여간내기가 아니군요」
				PRINTFORML 「후후훗、부끄러워하지 않아도 돼요」
				PRINTFORML 「그럼、제 여동생을 잘 부탁드립니다」
			;鈴仙orレイセンとデート
			ELSEIF FLAG:데이트상대 == 52 || FLAG:데이트상대 == 100
				PRINTFORML 「\@ FLAG:데이트상대 == 52 ? 레이센(鈴仙) # 레이센 \@、실수가 없도록 하세요」
				PRINTFORML 「무슨 일이 생기면、귀녀가 그를 보호하는 거야？」
				PRINTFORML 「후훗、좋은 답변이군요。그럼 다녀오세요」
			;それ以外
			ELSE
				PRINTFORML 「어머、데이트？」
				PRINTFORML 「후훗、즐기고 오세요」
			ENDIF
			PRINTFORMDW 토요히메는 살짝 손을 흔들어 데이트 중인 두 사람을 배웅했다
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;注、このあと通常の遭遇口上、ARG==1 OR ARG==2に接続します
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で一日最初の挨拶
	;ARG:1に데이트상대、ARG:1が0の場合は非デート
	;注、このあと通常の外出先で遭遇した口上、ARG==6 OR ARG==7に接続します
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;ARG:1とデート中
		IF ARG:1
		;一人で外出
		ELSE
		ENDIF
		PRINTFORML 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K99_2(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（연모時添い寝中就寝時のみ？
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(연모無しでも入ってくる可能性有り
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;１５分の１の確率で朝フェラ
		IF !RAND:15
			PRINTFORMDL 뭔가 간질간질한 감각에 눈을 뜨자……
			PRINTFORMDW 무려、토요히메가 %CALLNAME:MASTER%의 사타구니에 얼굴을 묻고 있었다
			CALL HPH_PRINT,@"「츄루……츄우우……후우……っHPH」","L"
			CALL HPH_PRINT,@"「응、후……쿠후……츄루룹……っHPH」","W"
			PRINTFORMDL 토요히메는 열심히 정성스레 페니스를 물고、핥고、빨고 있다
			PRINTFORMDW 이윽고 %CALLNAME:MASTER%가 사정하자、토요히메는 목을 크게 울리며 아침의 첫번째 착즙을 삼켰다
			PRINTFORMW 
			PRINTFORMDL 몇십분 후、%CALLNAME:MASTER%가 기상하자 토요히메는 아무렇지도 않은듯 아침인사를 해왔다
			PRINTFORMDW ………빠진 %CALLNAME:MASTER%의 음모가 입가에 달라붙은 채로………
			BASE:MASTER:精力 -= 50
		;通常
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「응……일어났어？」
					PRINTFORML 「후훗、잘 잤나요」
					PRINTFORMW 「도대체 무슨 꿈을 꾸고 있었던 거야？」
				CASE 1
					PRINTFORMDL %CALLNAME:MASTER%가 눈을 뜨자 토요히메가 옆에서 곁잠을 자고 있었다
					PRINTFORML 「안녕。%CALLNAME:MASTER%」
					PRINTFORML 「덥다고？ 후훗♪ 뭐 좋잖아♪」
					PRINTFORMDW 잠시 꽁냥꽁냥거린 후、%CALLNAME:MASTER%는 이불 밖으로 기어나왔다
				CASE 2
					PRINTFORMDL 깨어보니 토요히메가 %CALLNAME:MASTER%에게 걸터앉아 있었다
					PRINTFORML 「아앙……좀만 더 잠들어 있었으면 좋았는데……」
					PRINTFORMDW 대체 뭘 하려고 했을까……
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「새근……새근……」
		PRINTFORMDL %CALLNAME:MASTER%가 눈을 뜨자、토요히메는 아직 자고 있었다
		PRINTFORMDW 편안한 숨소리를 내는 그녀를 깨우지 않기 위해、%CALLNAME:MASTER%는 조심스레 이불에서 기어나왔다
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝時TARGET起床はこちら（TARGET쇠약or添い寝中TARGET睡魔負け後そのままMASTERが起き続けた上で相手が起床した場合限定
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;夜寝るとき（おやすみ口上）
;==================================================
@M_KOJO_EVENT_K99_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;自室に帰って寝る（～は【現在地】から【キャラの自室】に戻りました）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF ARG:1
			;デート中に眠くて帰って寝る（拠点キャラのみ）
			PRINTFORMW 
		ELSEIF CFLAG:우후후
			;우후후中に眠くて自室に帰って寝る（拠点キャラのみ）
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている（～は眠そうにしている）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;수면간中（～は眠っている）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;イタズラ中
	;記入チェック（=0, 非表示、1, 表示）
	;LOCAL:1 = 0
	;-------------------------------------------------
	;IF LOCAL:1 && ARG == 4
		;PRINTFORMW 
		;RETURN 1
	;ENDIF
	;-------------------------------------------------
	;쇠약して寝る（体力500以下,우후후時は우후후強制終了）
	;（キャラの自室以外：～を【現在地】から【キャラの自室】に運びました）
	;（キャラの自室　　：～は疲れたと言って、横になりました）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF CFLAG:우후후
			;우후후中にダウン
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（연모のみ,そのまま一日が終了）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		;お酒でダウン
		IF BASE:MASTER:술기운 >= MAXBASE:MASTER:술기운
			IF ARG:1
				;自室でダウン,（～が【MASTER】をベッド（布団）へと運んでくれた…）
				PRINTFORMW 
			ELSE
				;部屋まで運ばれる,（～が【MASTER】を私室へと運んでくれた…）
				PRINTFORMW 
			ENDIF
		;体力切れでダウン
		ELSE
			IF ARG:1
				;自室でダウン,（～が【MASTER】をベッド（布団）へと運んでくれた…）
				PRINTFORMW 
			ELSE
				;部屋まで運ばれる,（～が【MASTER】を私室へと運んでくれた…）
				PRINTFORMW 
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが寝るために自室まで戻ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;-------------------------------------------------
		;そのまま着替えて寝た（【TARGET】が部屋に戻ってきた→寝間着へと着替え、寝床に潜った）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 0
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;部屋から追い出して寝た（眠そうな【TARGET】に追い出された…）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			IF CFLAG:전턴위치 == CFLAG:현재위치
				;前のターンまで一緒に部屋にいた
				PRINTFORMW 
			ELSE
				;寝るために部屋に帰ってきたところ
				PRINTFORMW 
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;他キャラとのＨを止めさせて寝た（【TARGET】が部屋に戻ってきた→唇をとがらせる【TARGET】に、【MASTER】は渋々【Ｈの相手】から離れた）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
