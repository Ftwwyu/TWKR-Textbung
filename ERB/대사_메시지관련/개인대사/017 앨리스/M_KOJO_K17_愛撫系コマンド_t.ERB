	;플래그 첫회 체크
	;-----------------
	IF !CFLAG:K17C_ID:K17C_CF_FK마스터
		CALL K17_FIRSTKISS(TARGET)
		RETURN 1
	ENDIF
	SELECTCASE RAND:3
		CASE 0
			iABLnum = K17C_부위C
		CASE 1
			iABLnum = K17C_부위B
		CASE 2
			iABLnum = K17C_부위M
	ENDSELECT
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(), iABLnum)
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	PRINTW
ENDIF
RETURN 1
