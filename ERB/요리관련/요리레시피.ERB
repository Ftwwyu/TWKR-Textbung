@요리레시피검증
#FUNCTION
IF !재료첨가여부("주재료",RCP_주재료:0,RCP_주재료:1)
    RETURNF 0
ENDIF
FOR LOCAL,0,9
    SIF RCP_부재료:LOCAL:1 == 0 ;재료 0번은 로터니까...
        BREAK
    IF !재료첨가여부("부재료",RCP_부재료:LOCAL:0,RCP_부재료:LOCAL:1)
        IF RCP_부재료R1:LOCAL:1 != 0 && !재료첨가여부("부재료",RCP_부재료R1:LOCAL:0,RCP_부재료R1:LOCAL:1)
            IF RCP_부재료R2:LOCAL:1 != 0 && !재료첨가여부("부재료",RCP_부재료R2:LOCAL:0,RCP_부재료R2:LOCAL:1)
                RETURNF 0
            ENDIF
        ENDIF
    ENDIF
    SIF RCP_부재료:LOCAL:0 == -1
        BREAK
NEXT
FOR LOCAL,0,9
    SIF RCP_양념:LOCAL == 0
        BREAK
    IF !재료첨가여부("양념",RCP_양념:LOCAL,0)
        IF RCP_양념R1:LOCAL != 0 && !재료첨가여부("양념",RCP_양념R1:LOCAL,0)
            IF RCP_양념R2:LOCAL != 0 && !재료첨가여부("양념",RCP_양념R2:LOCAL,0)
                RETURNF 0
            ENDIF
        ENDIF
    ENDIF
    SIF RCP_양념:LOCAL == -1
        BREAK
NEXT
RETURNF 1

