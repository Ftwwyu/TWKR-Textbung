@CHARAMOVE_DATA_136(ARG,ARG:2)
LOCAL = 136
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;보류
			CASEELSE
				RETURN 0
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 5, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 21, 22, 23, 51, 52, 53, 56, 57, 71, 73, 81, 82, 83, 84
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터136
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터136(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "타카네")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "카스케트"))
CASE "평상복：상반신윗도리1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리1", "윗도리"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "가방"))


CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "특수 가공의")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "위장 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "미채 브래지어")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 3)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "야채/버섯/버섯/버섯/이파리/단/단/시큼한/시큼한/시원한/건강한/검소/예술적/야취/뭉근뭉근/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "개구리/개구리/개구리/생선 요리/생선 요리/생선 요리/짠/짠/푸짐/정키/수수께끼 요리/수수께끼 요리/수수께끼 요리/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "특기요리＊이파리＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "고급/기술/초록빛/질서/귀여운/시크/지적/매니악/" + "식물/진녹색의/" * 3 + "잎사귀의/숲의/지갑/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "푸른빛/파워풀/위엄/" + "싸구려/물/" * 3 + "캇파의/텐구의/오니의/손목호구/" * 10)
ENDSELECT

